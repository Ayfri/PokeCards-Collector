---
import type {App} from '../../env';
export async function GET() {
	const locals = Astro.locals as App.Locals;
	locals.user = null;

	return new Response('ok', {
		headers: {
			Location: '/',
			'Set-Cookie': 'session-token=; Path=/; HttpOnly; Max-Age=-1; SameSite=Strict; Secure',
		},
	});
}
